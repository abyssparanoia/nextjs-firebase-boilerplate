{
  "name": "abyssparanoia-electron",
  "version": "0.0.1",
  "main": "dist/main.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn build:prod",
    "build:dev": "webpack --config ./configs/webpack/webpack.config.dev.babel.ts",
    "build:stg": "webpack --config ./configs/webpack/webpack.config.stg.babel.ts",
    "build:prod": "webpack --config ./configs/webpack/webpack.config.prod.babel.ts",
    "build:mac:prod": "yarn build:prod && ts-node ./configs/builder/builder.mac.prod.ts",
    "build:mac:local": "yarn build:prod && ts-node ./configs/builder/builder.mac.local.ts",
    "build:win:prod": "yarn build:prod && ts-node ./configs/builder/builder.win.prod.ts",
    "build:icon": "npx @svgr/cli --icon -d src/renderer/components/nano/Icon/tsx src/renderer/components/nano/Icon/svgs --ext tsx --template src/renderer/components/nano/Icon/template.js",
    "start": "yarn build:dev && yarn start:server",
    "start:prod": "yarn build:prod && electron ./dist/main.js",
    "start:server": "webpack-dev-server --config ./configs/webpack/webpack.config.server.babel.ts",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint --fix -c ./.eslintrc.json './src/**/*.{ts,tsx}'",
    "test": "jest",
    "clean": "rimraf dist",
    "clean:app": "rimraf Electron"
  },
  "engine": {
    "node": "12.x",
    "yarn": "^1.16.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/register": "7.12.1",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/addon-backgrounds": "5.3.21",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-links": "5.3.21",
    "@storybook/addons": "5.3.21",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.21",
    "@testing-library/react": "11.1.0",
    "@types/electron-load-devtool": "^1.0.0",
    "@types/history": "4.7.7",
    "@types/jest": "25.2.3",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-redux": "7.1.10",
    "@types/react-router": "5.1.8",
    "@types/redux-mock-store": "1.0.2",
    "@types/webpack-env": "1.15.3",
    "babel-loader": "8.1.0",
    "electron": "10.1.3",
    "electron-builder": "21.2.0",
    "electron-load-devtool": "^1.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "identity-obj-proxy": "3.0.0",
    "react-hot-loader": "4.13.0",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "source-map-loader": "1.1.2",
    "ts-loader": "8.0.7",
    "ts-node": "9.0.0",
    "webpack": "4.44.2",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.10.3",
    "webpack-jarvis": "0.3.2",
    "webpack-merge": "5.3.0"
  },
  "dependencies": {
    "@abyssparanoia/interface": "0.0.1",
    "@babel/preset-env": "^7.7.1",
    "@material-ui/core": "^4.9.7",
    "babel-plugin-import": "^1.12.2",
    "connected-react-router": "^6.7.0",
    "css-loader": "^5.0.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "query-string": "^6.11.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "style-loader": "^2.0.0",
    "use-image": "^1.0.5"
  }
}
