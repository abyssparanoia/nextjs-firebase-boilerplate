{
  "name": "@abyssparanoia/graphql",
  "version": "0.0.1",
  "private": true,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "scripts": {
    "build": "graphql-codegen --config codegen.yml && run-p -l build:* && yarn format",
    "build:cjs": "tsc --project . --module commonjs --outDir ./cjs",
    "build:esm": "tsc --project . --module es2015 --outDir ./esm",
    "clean": "rimraf esm cjs",
    "lint": "tsc -p . --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prebuild": "yarn clean"
  },
  "dependencies": {
    "@graphql-codegen/cli": "1.13.4",
    "@graphql-codegen/typescript": "1.13.4",
    "@graphql-codegen/typescript-operations": "1.13.4",
    "@graphql-codegen/typescript-react-apollo": "1.13.4",
    "react-apollo": "3.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.4",
    "@graphql-codegen/introspection": "1.13.4",
    "@graphql-codegen/typescript": "1.13.4",
    "@graphql-codegen/typescript-operations": "1.13.4",
    "@graphql-codegen/typescript-react-apollo": "1.13.4",
    "@graphql-codegen/typescript-resolvers": "1.13.4"
  }
}
